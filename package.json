{
  "name": "dcbor-envelope",
  "displayName": "dCBOR-Envelope",
  "description": "Syntax highlighting for dCBOR envelopes",
  "version": "0.0.1",
  "publisher": "local",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Other"],
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "test": "jest"
  },
  "keywords": ["syntax", "dcbor", "envelope"],
  "license": "MIT",
  "type": "commonjs",
  "contributes": {
    "languages": [
      {
        "id": "dcbor-envelope",
        "aliases": ["dCBOR-Envelope", "dcbor", "envelope"],
        "extensions": [".dcbor", ".envelope"],
        "configuration": "./language-configuration.json"
      }
    ],
      "grammars": [
        {
          "language": "dcbor-envelope",
          "scopeName": "source.dcbor-envelope",
          "path": "./syntaxes/dcbor-envelope.tmLanguage.json"
        }
      ],
      "themes": [
        {
          "label": "dCBOR Envelope Theme",
          "uiTheme": "vs-dark",
          "path": "./themes/dcbor-envelope-color-theme.json"
        }
      ]
    },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/vscode": "^1.75.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.5",
    "vscode-textmate": "^9.2.0",
    "vscode-oniguruma": "^2.0.1"
  }
}
