{
    "name": "dCBOR Envelope Light Theme",
    "include": "./light_plus.json",
    "type": "light",
    "colors": {},
    "tokenColors": [
        {
            "scope": [
                "string.quoted.double",
                "meta.embedded.block string.quoted.double",
                "source string.quoted.double"
            ],
            "settings": {
                "foreground": "#A31515"
            }
        },
        {
            "scope": [
                "string.quoted.single",
                "meta.embedded.block string.quoted.single",
                "source string.quoted.single"
            ],
            "settings": {
                "foreground": "#A31515"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "meta.embedded.block constant.numeric",
                "source constant.numeric"
            ],
            "settings": {
                "foreground": "#098658"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "meta.embedded.block entity.name.function",
                "source entity.name.function"
            ],
            "settings": {
                "foreground": "#795E26"
            }
        },
        {
            "scope": [
                "keyword",
                "meta.embedded.block keyword",
                "source keyword"
            ],
            "settings": {
                "foreground": "#0000FF"
            }
        },
        {
            "scope": [
                "comment",
                "meta.embedded.block comment",
                "source comment"
            ],
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "scope": [
                "storage.type",
                "meta.embedded.block storage.type",
                "source storage.type"
            ],
            "settings": {
                "foreground": "#0000FF"
            }
        },
        {
            "scope": [
                "string.quoted.double.dcbor",
                "meta.embedded.block.envelope string.quoted.double",
                "meta.embedded.block.dcbor string.quoted.double",
                "meta.embedded.block.cbor string.quoted.double"
            ],
            "settings": {
                "foreground": "#0b631e"
            }
        },
        {
            "scope": "string.quoted.single.dcbor, constant.other.unit.dcbor",
            "settings": {
                "foreground": "#529721"
            }
        },
        {
            "scope": "string.quoted.prefixed.dcbor, string.quoted.prefixed.multiline.dcbor, entity.name.tag.dcbor",
            "settings": {
                "foreground": "#0D7D65"
            }
        },
        {
            "scope": [
                "string.regexp",
                "string.regexp.envelope-pattern"
            ],
            "settings": {
                "foreground": "#b83838"
            }
        },
        {
            "scope": "keyword.operator.envelope-pattern",
            "settings": {
                "foreground": "#529815"
            }
        },
        {
            "scope": [
                "variable.annotation.envelope-pattern",
                "punctuation.definition.annotation.envelope-pattern"
            ],
            "settings": {
                "foreground": "#c051a2"
            }
        },
        {
            "scope": "keyword.other.envelope-pattern",
            "settings": {
                "foreground": "#1661e1"
            }
        },
        {
            "scope": "constant.numeric.dcbor, constant.numeric.special.dcbor, constant.numeric.hex.dcbor, constant.numeric.binary.dcbor, constant.numeric.octal.dcbor",
            "settings": {
                "foreground": "#0E76B0"
            }
        },
        {
            "scope": "constant.numeric.date.dcbor",
            "settings": {
                "foreground": "#0D7D65"
            }
        },
        {
            "scope": "punctuation.separator.dcbor",
            "settings": {
                "foreground": "#0E76B0"
            }
        },
        {
            "scope": "punctuation.square.brackets.dcbor",
            "settings": {
                "foreground": "#333333"
            }
        },
        {
            "scope": "punctuation.angle.brackets.double.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "punctuation.angle.brackets.guillemet.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "punctuation.angle.brackets.floral.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "meta.embedded.block.dcbor punctuation.square.brackets.dcbor, meta.embedded.block.envelope punctuation.square.brackets.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "meta.embedded.block.dcbor punctuation.curly.braces.dcbor, meta.embedded.block.envelope punctuation.curly.braces.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "meta.embedded.block.dcbor punctuation.parenthesis.dcbor, meta.embedded.block.envelope punctuation.parenthesis.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "meta.embedded.block.dcbor punctuation.angle.brackets.double.dcbor, meta.embedded.block.envelope punctuation.angle.brackets.double.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "meta.embedded.block.dcbor punctuation.angle.brackets.guillemet.dcbor, meta.embedded.block.envelope punctuation.angle.brackets.guillemet.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "meta.embedded.block.dcbor punctuation.angle.brackets.floral.dcbor, meta.embedded.block.envelope punctuation.angle.brackets.floral.dcbor",
            "settings": {
                "foreground": "#886200"
            }
        },
        {
            "scope": "constant.language.dcbor",
            "settings": {
                "foreground": "#5D3CA9"
            }
        },
        {
            "scope": "keyword.other.envcase.dcbor",
            "settings": {
                "foreground": "#A13030"
            }
        },
        {
            "scope": "keyword.other.treebranch.dcbor",
            "settings": {
                "foreground": "#935692"
            }
        },
        {
            "scope": "constant.other.ur.dcbor",
            "settings": {
                "foreground": "#3D724C"
            }
        },
        {
            "scope": "identifier.bareword.dcbor",
            "settings": {
                "foreground": "#9e5c1a"
            }
        },
        {
            "scope": [
                "comment.line.number-sign.dcbor",
                "comment.block.inline.dcbor",
                "comment.line.ellipsis.dcbor"
            ],
            "settings": {
                "foreground": "#979696"
            }
        }
    ]
}
